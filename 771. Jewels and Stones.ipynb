   "source": [
    "def sol(jewels, stones):\n",
    "\n",
    "    jewels = set(jewels)\n",
    "\n",
    "    c = 0\n",
    "\n",
    "    for i in stones:\n",
    "        if i in jewels:\n",
    "            c += 1\n",
    "\n",
    "    return c"
   ]
